{
  "name": "boa-org",
  "version": "1.0.0",
  "description": "BOA Organization - Frontend & Backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd boa-connect && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd boa-connect && npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}